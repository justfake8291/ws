
cal.cs 
code:

public int add(int a, int b)
        {
            return a + b;
        }
        public int mul(int a, int b)
        {
            return a * b;
        }


Ical.cs :

        [OperationContract]
        int add(int a, int b);
        [OperationContract]
        int  mul (int a, int b);
